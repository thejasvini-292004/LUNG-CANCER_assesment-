{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Chrome",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}",
        "runtimeExecutable": "C:/Program Files/Google/Chrome/Application/chrome.exe", // Ensure this path is correct for your system
        "userDataDir": "${workspaceFolder}/.vscode/chrome"
      }
    ]
  }
  